<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#f2f2f2"
    android:baselineAligned="false"
    android:fitsSystemWindows="true"
    android:weightSum="3" >

    <RelativeLayout
        android:id="@+id/relative1"
        android:layout_width="0dp"
        android:layout_height="fill_parent"
        android:layout_weight="1"
        android:padding="20dp" >
        <ImageButton
            android:id="@+id/imageButtonMulti2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/multi"
            />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/imageButtonMulti2"
            android:orientation="vertical"
            android:weightSum="16" >

            <ImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/holo_round_button"
                android:scaleType="fitXY"
                android:src="@drawable/plus_icon"
                android:padding="16dp"
                android:layout_above="@+id/text_multi_0"
                android:id="@+id/plus_multi_0"
                android:layout_centerInParent="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text=""
                android:layout_above="@+id/minus_multi_0"
                android:layout_centerInParent="true"
                android:textColor="#2f2f2f"
                android:id="@+id/text_multi_0" />

            <ImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/holo_round_button"
                android:scaleType="fitXY"
                android:layout_gravity="center"
                android:src="@drawable/minus_icon"
                android:layout_alignParentBottom="true"
                android:padding="16dp"
                android:layout_marginBottom="16dp"
                android:layout_centerInParent="true"
                android:id="@+id/minus_multi_0" />

        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relative2"
        android:layout_width="0dp"
        android:layout_height="fill_parent"
        android:layout_weight="1"
        android:padding="20dp" >
         <ImageButton
                android:id="@+id/imageButtonPhone2"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="10dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/phone" >
        </ImageButton>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/imageButtonPhone2"
            android:orientation="vertical"
            android:weightSum="16" >

            <ImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/holo_round_button"
                android:scaleType="fitXY"
                android:src="@drawable/plus_icon"
                android:padding="16dp"
                android:layout_above="@+id/text_phone_1"
                android:id="@+id/plus_phone_1"
                android:layout_centerInParent="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text=""
                android:layout_above="@+id/minus_phone_1"
                android:layout_centerInParent="true"
                android:textColor="#2f2f2f"
                android:id="@+id/text_phone_1" />

            <ImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/holo_round_button"
                android:scaleType="fitXY"
                android:layout_gravity="center"
                android:src="@drawable/minus_icon"
                android:layout_alignParentBottom="true"
                android:padding="16dp"
                android:layout_marginBottom="16dp"
                android:layout_centerInParent="true"
                android:id="@+id/minus_phone_1" />
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relative3"
        android:layout_width="0dp"
        android:layout_height="fill_parent"
        android:layout_weight="1"
        android:padding="20dp" >
        <ImageButton
            android:id="@+id/imageButtonAlarm2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/alarm" >
        </ImageButton>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/imageButtonAlarm2"
            android:orientation="vertical"
            android:weightSum="16" >

            <ImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/holo_round_button"
                android:scaleType="fitXY"
                android:src="@drawable/plus_icon"
                android:padding="16dp"
                android:layout_above="@+id/text_alarm_2"
                android:id="@+id/plus_alarm_2"
                android:layout_centerInParent="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text=""
                android:textColor="#2f2f2f"
                android:layout_above="@+id/minus_alarm_2"
                android:layout_centerInParent="true"
                android:id="@+id/text_alarm_2" />

            <ImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/holo_round_button"
                android:scaleType="fitXY"
                android:layout_gravity="center"
                android:src="@drawable/minus_icon"
                android:layout_alignParentBottom="true"
                android:padding="16dp"
                android:layout_marginBottom="16dp"
                android:layout_centerInParent="true"
                android:id="@+id/minus_alarm_2" />
        </RelativeLayout>

    </RelativeLayout>

</LinearLayout>